#include<iostream>
#include<math.h>
using namespace std;

int main()
{
	int a, b;
	cout << "Nhap a: "; cin >> a;
	cout << "Nhap b: "; cin >> b;

	int Tong = a + b;
	int Hieu = a - b;
	long Tich = a * b;
	double Thuong = (double)a / b;
	int Chiadu = a % b;

	cout << "a + b = " << Tong << endl;
	cout << "a - b = " << Hieu << endl;
	cout << "a * b = " << Tich << endl;
	cout << "a / b = " << Thuong << endl;
	cout << "a % b = " << Chiadu << endl;
	return 0;

}
