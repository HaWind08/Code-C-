#include<iostream>
#include<math.h>

using namespace std;

int main()
{
	int a, b; cin >> a >> b;
	int tong = a + b;
	int hieu = a - b;
	long long tich = a * b;
	float thuong = (float)a / b;
	float chiadu = a % b;

	cout << tong << endl;
	cout << hieu << endl;
	cout << tich << endl;
	cout << thuong << endl;
	cout << chiadu << endl;

	return 0;

}
